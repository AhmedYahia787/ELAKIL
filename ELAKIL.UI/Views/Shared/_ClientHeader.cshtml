@{var controllerName = string.Empty; }
@{object controllerObj; }

@if (ViewContext.RouteData.Values.TryGetValue("area", out controllerObj))
{
    controllerName = controllerObj.ToString();
}

@if (controllerName == "Identity")
{
    <partial name="_TopAnchorsIdentity" />
}
else
{
    <partial name="_TopAnchorsNotIdentity" />
}